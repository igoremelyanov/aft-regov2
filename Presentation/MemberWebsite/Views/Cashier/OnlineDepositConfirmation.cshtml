@{
    ViewBag.Title = "My account - Rego";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string section = "onlinedeposit";
}

<section class="main-content section account">

    <div class="container">

        <h2 class="title-page"><span>Cashier</span></h2>

        <div class="row row-eq-height">

            @Html.Partial("_PartialCashierMenu", section)

            <div class="col-sm-9 white-content withdrawal" id="withdrawal-wrapper">

                <h2 data-i18="balanceInformation.onlineDeposit">online deposit</h2>
                
                <h5 class="section"><span data-i18n="">summary confirmation</span></h5>

                <table class="tableinfo">
                    <tr>
                        <td><b data-i18n="balanceInformation.onlineDeposit"></b></td>
                        <td>$50,000.00</td>
                    </tr>
                    <tr>
                        <td><b data-i18n="balanceInformation.bonusCode"></b></td>
                        <td>ACDC69420 50% Deposit Bonus</td>
                    </tr>
                    <tr>
                        <td><b data-i18n="balanceInformation.total">Total</b></td>
                        <td>$75,000.00</td>
                    </tr>
                </table>
                
                @Html.Partial("_PartialQuickLink")

            </div>
        </div>
    </div>

</section>

@section scripts {
    <script src="/Scripts/custom/login.js"></script>
    <script src="/Scripts/custom/formbase.js"></script>
    <script src="/Scripts/custom/withdrawal.js"></script>
   
}